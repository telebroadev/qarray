{
  "name": "telebroadev/qarray",
  "description": "QArray is a PHP abstraction for querying array",
  "keywords": ["array", "query", "php"],
  "homepage": "https://github.com/telebroadev/qarray",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "name": "Nahid Bin Azhar",
    "email": "nahid.dns@gmail.com"
  }],
  "repositories": [{
    "type": "vcs",
    "url": "https://github.com/fsevestre/DeepCopy"
  }],
  "require": {
    "php": ">=5.6",
    "myclabs/deep-copy": "1.5",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0 || ^6.0",
    "symfony/var-dumper": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Nahid\\QArray\\": "src/"
    },
    "files": [
      "helpers/qarray.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nahid\\JsonQ\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": false
}